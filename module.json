{
  "id": "foundryvtt-simple-calendar-compat",
  "title": "Simple Calendar Compatibility Bridge",
  "description": "Provides Simple Calendar API compatibility for modern calendar modules like Seasons & Stars",
  "version": "1.0.0",
  "authors": [
    {
      "name": "Rayners",
      "discord": "rayners"
    }
  ],
  "compatibility": {
    "minimum": "12",
    "verified": "13",
    "maximum": "13"
  },
  "esmodules": [
    "main.js"
  ],
  "languages": [
    {
      "lang": "en",
      "name": "English",
      "path": "languages/en.json"
    }
  ],
  "relationships": {
    "requires": [],
    "recommends": [
      {
        "id": "seasons-and-stars",
        "type": "module",
        "reason": "Primary calendar provider for compatibility bridging"
      }
    ],
    "conflicts": [
      {
        "id": "foundryvtt-simple-calendar",
        "type": "module",
        "reason": "Provides the same API - use either this bridge or Simple Calendar, not both"
      }
    ]
  },
  "socket": false,
  "manifest": "https://github.com/rayners/foundryvtt-simple-calendar-compat/releases/latest/download/module.json",
  "download": "https://github.com/rayners/foundryvtt-simple-calendar-compat/releases/latest/download/module.zip",
  "license": "MIT",
  "readme": "https://github.com/rayners/foundryvtt-simple-calendar-compat/blob/main/README.md",
  "changelog": "https://github.com/rayners/foundryvtt-simple-calendar-compat/blob/main/CHANGELOG.md"
}