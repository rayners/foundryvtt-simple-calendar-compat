{
  "name": "foundryvtt-simple-calendar-compat",
  "version": "0.1.2",
  "description": "Simple Calendar API compatibility bridge for modern calendar modules",
  "type": "module",
  "main": "dist/main.js",
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "test": "vitest",
    "test:run": "vitest run",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write \"src/**/*.{ts,js,json}\" \"*.{js,json,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,js,json}\" \"*.{js,json,md}\"",
    "typecheck": "tsc --noEmit",
    "validate": "npm run lint && npm run format:check && npm run typecheck && npm run test:run && npm run build",
    "prepare": "npm run build",
    "clean": "rimraf dist"
  },
  "keywords": [
    "foundry-vtt",
    "calendar",
    "compatibility",
    "simple-calendar",
    "bridge"
  ],
  "author": "Rayners",
  "license": "MIT",
  "devDependencies": {
    "@rollup/plugin-typescript": "^11.1.5",
    "@types/jquery": "^3.5.32",
    "@types/node": "^24.0.1",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0",
    "@vitest/coverage-v8": "^1.0.0",
    "@vitest/ui": "^1.0.0",
    "eslint": "^8.50.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jsdom": "^26.1.0",
    "prettier": "^3.1.0",
    "rimraf": "^5.0.5",
    "rollup": "^4.0.0",
    "tslib": "^2.6.2",
    "typescript": "^5.2.2",
    "vitest": "^1.0.0"
  },
  "peerDependencies": {
    "seasons-and-stars": "*"
  },
  "peerDependenciesMeta": {
    "seasons-and-stars": {
      "optional": true
    }
  },
  "files": [
    "dist/",
    "module.json",
    "languages/",
    "README.md",
    "CHANGELOG.md"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/rayners/foundryvtt-simple-calendar-compat.git"
  },
  "bugs": {
    "url": "https://github.com/rayners/foundryvtt-simple-calendar-compat/issues"
  },
  "homepage": "https://github.com/rayners/foundryvtt-simple-calendar-compat#readme"
}
